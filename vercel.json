{
  "rewrites": [
    {
      "source": "/blog/:slug",
      "destination": "https://service.prerender.io/blog/:slug"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/blog/:slug",
      "headers": [
        { "key": "X-Prerender-Token", "value": "CL9qxdFMjwTK8IXeCAod" }
      ]
    }
  ]
}
